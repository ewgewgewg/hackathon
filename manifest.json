{
    "manifest_version": 2,
    "name": "HyperDoge",
    "version": "1.0",
    "permissions": [
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "webRequest",
        "webRequestBlocking"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["./main.js"]
        }
    ]
  }
