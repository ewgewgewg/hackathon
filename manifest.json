{
    "manifest_version": 2,
    "name": "Focus",
    "version": "1.0",
    "content_scripts": [
        {
          "matches":  ["<all_urls>"],
          "js": ["./main.js"]
        }
      ],
      "permissions": [
        "audioCapture",
        "background",
        "browser",
        "geolocation",
        "storage",
        "unlimitedStorage",
        "videoCapture",
        "tabs"
      ]
  }
